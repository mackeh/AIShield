# Example .env file for aishield-analytics

# Database connection string (PostgreSQL with TimescaleDB)
DATABASE_URL=postgres://aishield:aishield_dev_password@localhost:5432/aishield_analytics

# API key for authentication (change this in production!)
AISHIELD_API_KEY=dev_key_please_change_in_production

# Server port
PORT=8080

# Logging level
RUST_LOG=info,aishield_analytics=debug
